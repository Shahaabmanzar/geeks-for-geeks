# Python 3 code to find sum of elements in given array 
def sum(arr,n): 
      
    # return sum using sum inbuilt sum() function 
    return(sum(arr)) 
  
# driver function 
arr=[] 
# input values to list 
arr = [12, 3, 4, 15] 
  
# to calculate length of array 
n = len(arr) 
  
ans = sum(arr,n) 
  
# display sum 
print (\"Sum of the array is \",ans) 
